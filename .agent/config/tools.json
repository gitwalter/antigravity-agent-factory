{
  "tools": {
    "gh": {
      "description": "GitHub CLI executable",
      "env_var": "GH_CLI_PATH",
      "fallback_paths": [
        "C:\\Program Files\\GitHub CLI\\gh.exe",
        "/usr/local/bin/gh",
        "/usr/bin/gh"
      ]
    },
    "git": {
      "description": "Git executable",
      "env_var": "GIT_PATH",
      "fallback_paths": [
        "C:\\Program Files\\Git\\cmd\\git.exe",
        "/usr/bin/git",
        "/usr/local/bin/git"
      ]
    }
  }
}