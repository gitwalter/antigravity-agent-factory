{
  "tools": {
    "gh": {
      "description": "GitHub CLI executable",
      "env_var": "GH_CLI_PATH",
      "fallback_paths": [
        "C:\\Program Files\\GitHub CLI\\gh.exe",
        "/usr/local/bin/gh",
        "/usr/bin/gh"
      ]
    },
    "atlassian_mcp": {
      "description": "Atlassian MCP Server endpoint",
      "env_var": "ATLASSIAN_MCP_URL",
      "default_url": "https://mcp.atlassian.com/v1/sse"
    },
    "git": {
      "description": "Git executable",
      "env_var": "GIT_PATH",
      "fallback_paths": [
        "C:\\Program Files\\Git\\bin\\git.exe",
        "C:\\Program Files\\Git\\cmd\\git.exe",
        "/usr/bin/git",
        "/usr/local/bin/git"
      ]
    },
    "jira": {
      "description": "Jira Cloud base URL",
      "env_var": "JIRA_BASE_URL",
      "default_url": "https://company.atlassian.net"
    },
    "linear": {
      "description": "Linear App base URL",
      "env_var": "LINEAR_BASE_URL",
      "default_url": "https://linear.app"
    },
    "python": {
      "description": "Python executable",
      "env_var": "PYTHON_PATH",
      "fallback_paths": [
        "D:\\Anaconda\\envs\\langgraph-gemini\\python.exe",
        "C:\\App\\Anaconda\\envs\\langgraph-gemini\\python.exe",
        "D:\\Anaconda\\python.exe",
        "C:\\App\\Anaconda\\python.exe"
      ]
    },
    "pip": {
      "description": "Pip executable",
      "env_var": "PIP_PATH",
      "fallback_paths": [
        "D:\\Anaconda\\envs\\langgraph-gemini\\Scripts\\pip.exe",
        "C:\\App\\Anaconda\\envs\\langgraph-gemini\\Scripts\\pip.exe",
        "D:\\Anaconda\\Scripts\\pip.exe",
        "C:\\App\\Anaconda\\Scripts\\pip.exe"
      ]
    },
    "conda": {
      "description": "Conda executable",
      "env_var": "CONDA_PATH",
      "fallback_paths": [
        "D:\\Anaconda\\Scripts\\conda.exe",
        "C:\\App\\Anaconda\\Scripts\\conda.exe"
      ]
    },
    "pytest": {
      "description": "Pytest executable",
      "env_var": "PYTEST_PATH",
      "fallback_paths": [
        "D:\\Anaconda\\envs\\langgraph-gemini\\Scripts\\pytest.exe",
        "C:\\App\\Anaconda\\envs\\langgraph-gemini\\Scripts\\pytest.exe"
      ]
    },
    "github_tokens_url": {
      "description": "URL to generate GitHub tokens",
      "env_var": "GITHUB_TOKENS_URL",
      "default_url": "https://github.com/settings/tokens"
    },
    "npm_tokens_url": {
      "description": "URL to generate NPM tokens",
      "env_var": "NPM_TOKENS_URL",
      "default_url": "https://www.npmjs.com/settings/tokens"
    },
    "anthropic_mcp_docs_url": {
      "description": "Anthropic MCP documentation URL",
      "env_var": "ANTHROPIC_MCP_DOCS_URL",
      "default_url": "https://docs.anthropic.com/claude/mcp"
    },
    "pythea_repo_url": {
      "description": "Pythea/Strawberry repository URL",
      "env_var": "PYTHEA_REPO_URL",
      "default_url": "https://github.com/leochlon/pythea"
    },
    "langchain_agents_docs_url": {
      "description": "LangChain agents documentation URL",
      "env_var": "LANGCHAIN_AGENTS_DOCS_URL",
      "default_url": "https://langchain.dev/docs/agents"
    }
  }
}