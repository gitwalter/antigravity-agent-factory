    "problem": "Developers need quick answers without reading full documentation",
    "solution": "Single-page reference for common ASP operations",
    "benefit": "Get working code in 30 seconds"
  },
  "quick_start": {
    "minimal_society": {
      "description": "Create a verified multi-agent society in 3 lines",
      "code": "from lib.society.simple import create_agent_society\n\nsociety = create_agent_society(\"MyProject\", agents=[\"orchestrator\", \"worker\"])\nresult = society.send(\"orchestrator\", \"worker\", {\"task\": \"analyze\"})"
    },
    "with_contract": {
      "description": "Add a formal contract between agents",
      "code": "contract = society.create_contract(\n    \"analysis-contract\",\n    parties=[(\"orchestrator\", \"delegator\"), (\"worker\", \"analyzer\")],\n    obligations=[(\"worker\", \"respond_within\", {\"timeout\": 60})],\n    prohibitions=[(\"worker\", \"modify_files\")]\n)"
    },
    "check_status": {
      "description": "Check agent reputation and trust level",
      "code": "status = society.get_agent_status(\"worker\")\nprint(f\"Reputation: {status.reputation_score}\")\nprint(f\"Trust Level: {status.trust_level}\")"
    }
  },
  "api_reference": {
    "SimpleSociety": {
      "create": "create_agent_society(name, agents=[...], preset=DEVELOPMENT)",
      "add_agent": "society.add_agent(agent_id, agent_type='general')",
      "send": "society.send(from_agent, to_agent, payload, message_type='REQUEST')",
      "broadcast": "society.broadcast(from_agent, payload, exclude=[...])",
      "on_message": "society.on_message(agent_id, handler_function)",
      "get_reputation": "society.get_reputation(agent_id) -> float",
      "get_trust_level": "society.get_trust_level(agent_id) -> str",
      "create_contract": "society.create_contract(id, parties, capabilities, obligations, prohibitions)",
      "get_stats": "society.get_stats() -> dict",
      "export_audit_log": "society.export_audit_log() -> dict"
    },
    "presets": {
      "create_supervisor_worker_society": "Supervisor delegates to workers",
      "create_peer_society": "Equal peers collaborate",
      "create_pipeline_society": "Sequential processing stages",
      "create_hierarchical_society": "Multi-level management"
    },
    "pabp": {
      "create_bundle": "create_bundle(agent_id, agent_name, version)",
      "add_skill": "bundle.add_skill(name, content)",
      "add_knowledge": "bundle.add_knowledge(name, content_dict)",
      "export_bundle": "export_bundle(bundle, output_path, compress=True)",
      "import_bundle": "import_bundle(source_path) -> (bundle, result)",
      "verify_bundle": "verify_bundle(bundle, public_key) -> result"
    }
  },
