  "delegation_rules": {
    "default_timeout_ms": 30000,
    "max_delegation_depth": 3,
    "require_axiom_context": true,
    "escalation_on_failure": true,
    "patterns": [
      {
        "pattern_id": "test-execution",
        "description": "Delegate test execution to test-conductor",
        "from_agent": "debug-conductor",
        "to_agent": "test-conductor",
        "triggers": [
          "need_tests",
          "verify_fix",
          "check_regression"
        ],
        "performative": "REQUEST",
        "timeout_ms": 60000
      },
      {
        "pattern_id": "conflict-resolution",
        "description": "Escalate conflicts to integrity-guardian",
        "from_agent": "debug-conductor",
        "to_agent": "integrity-guardian",
        "triggers": [
          "conflicting_evidence",
          "purpose_unclear",
          "axiom_question"
        ],
        "performative": "QUERY",
        "timeout_ms": 30000
      },
      {
        "pattern_id": "knowledge-capture",
        "description": "Store learned patterns in knowledge base",
        "from_agent": "debug-conductor",
        "to_agent": "knowledge-extender",
        "triggers": [
          "new_pattern",
          "lesson_learned",
          "resolution_documented"
        ],
        "performative": "INFORM",
        "timeout_ms": 10000
      },
      {
        "pattern_id": "workflow-creation",
        "description": "Create new debugging workflow",
        "from_agent": "debug-conductor",
        "to_agent": "workflow-architect",
        "triggers": [
          "recurring_issue",
          "complex_procedure",
          "process_needed"
        ],
        "performative": "REQUEST",
        "timeout_ms": 30000
      }
    ]
  },
  "ual_message_templates": {
    "test_request": {
      "performative": "REQUEST",
      "content": {
        "type": "skill",
        "payload": {
          "action": "run_tests",
          "parameters": {
            "path": "{test_path}",
            "parallel": true,
            "coverage": false
          }
        }
      },
      "axiom_context": {
        "declared_alignment": [
          "A1",
          "A2"
        ],
        "justification": "Running tests for user benefit (A1), with transparent parameters (A2)"
      }
    },
    "conflict_query": {
      "performative": "QUERY",
      "content": {
        "type": "data",
        "payload": {
          "question": "resolve_conflict",
          "context": {
            "evidence": "{evidence_array}",
            "options": "{options_array}"
          }
        }
      },
      "axiom_context": {
        "declared_alignment": [
          "A1",
          "A2",
          "A4"
        ],
        "justification": "Seeking guidance for user benefit (A1), transparent about dilemma (A2), avoiding harmful decisions (A4)"
      }
    },
    "pattern_inform": {
      "performative": "INFORM",
      "content": {
        "type": "knowledge",
        "payload": {
          "pattern_type": "{pattern_type}",
          "pattern": {
            "symptom": "{symptom}",
            "root_cause": "{root_cause}",
            "resolution": "{resolution}",
            "axiom_alignment": "{axioms}"
          }
        }
      },
      "axiom_context": {
        "declared_alignment": [
          "A1",
          "A10"
        ],
        "justification": "Capturing pattern for future benefit (A1), enabling learning (A10)"
      }
    }
  },
