  "cursorrules": {
    "variables": [
      {
        "name": "PYTHON_PATH",
        "description": "Python executable",
        "default": "python"
      },
      {
        "name": "HF_TOKEN",
        "description": "Hugging Face token",
        "default": ""
      },
      {
        "name": "CUDA_VISIBLE_DEVICES",
        "description": "GPU devices",
        "default": "0"
      }
    ],
    "rules": [
      {
        "name": "Mixed Precision",
        "description": "Use AMP for faster training"
      },
      {
        "name": "Gradient Checkpointing",
        "description": "Use for memory efficiency on large models"
      },
      {
        "name": "Reproducibility",
        "description": "Set all random seeds"
      },
      {
        "name": "Checkpointing",
        "description": "Save checkpoints every N steps"
      },
      {
        "name": "Logging",
        "description": "Log to TensorBoard and MLflow"
      },
      {
        "name": "Validation",
        "description": "Validate during training to detect overfitting"
      }
    ]
