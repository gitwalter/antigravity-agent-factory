  "cursorrules": {
    "variables": [
      {
        "name": "PYTHON_PATH",
        "description": "Python executable",
        "default": "python"
      },
      {
        "name": "OPENAI_API_KEY",
        "description": "OpenAI API key",
        "default": ""
      },
      {
        "name": "ANTHROPIC_API_KEY",
        "description": "Anthropic API key",
        "default": ""
      },
      {
        "name": "DEFAULT_LLM_MODEL",
        "description": "Default LLM model for generation",
        "default": "gpt-4o-mini"
      },
      {
        "name": "BATCH_SIZE",
        "description": "Batch size for data generation",
        "default": "10"
      },
      {
        "name": "VALIDATION_ENABLED",
        "description": "Enable data validation",
        "default": "true"
      }
    ],
    "rules": [
      {
        "name": "Structured Output",
        "description": "Use Instructor or Pydantic models for structured LLM output"
      },
      {
        "name": "Schema Validation",
        "description": "Always validate generated data against defined schemas"
      },
      {
        "name": "Privacy Preservation",
        "description": "Ensure synthetic data doesn't leak real data patterns"
      },
      {
        "name": "Batch Processing",
        "description": "Process data generation in batches for efficiency"
      },
      {
        "name": "Quality Metrics",
        "description": "Track quality metrics: realism, diversity, privacy"
      },
      {
        "name": "Reproducibility",
        "description": "Use seeds and version control for reproducible generation"
      },
      {
        "name": "Error Handling",
        "description": "Handle LLM failures gracefully with retries and fallbacks"
      },
      {
        "name": "Type Safety",
        "description": "Use type hints and Pydantic for all data structures"
      }
    ]
  },
