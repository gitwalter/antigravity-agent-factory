{
  "metadata": {
    "blueprintId": "ai-agent-development",
    "blueprintName": "AI Agent Development Blueprint",
    "description": "Build AI agent systems with LangChain, LangGraph, and modern LLM frameworks",
    "version": "2.0.0",
    "author": "Antigravity Agent Factory",
    "tags": [
      "python",
      "ai",
      "agents",
      "langchain",
      "langgraph",
      "llm",
      "ml"
    ],
    "purpose": "Enable developers to create intelligent, coordinated AI agent systems that serve users with integrity"
  },
  "stack": {
    "primaryLanguage": "python",
    "runtime": "3.13+",
    "frameworks": [
      {
        "name": "LangChain",
        "version": "1.2+",
        "purpose": "Agent framework and chains"
      },
      {
        "name": "LangGraph",
        "version": "1.0+",
        "purpose": "Stateful agent orchestration"
      },
      {
        "name": "CrewAI",
        "version": "1.9+",
        "purpose": "Multi-agent crew orchestration"
      },
      {
        "name": "AutoGen",
        "version": "0.11+",
        "purpose": "Conversational multi-agent systems"
      },
      {
        "name": "Streamlit",
        "version": "1.54+",
        "purpose": "Interactive UI"
      },
      {
        "name": "FastAPI",
        "version": "0.128+",
        "purpose": "API endpoints"
      },
      {
        "name": "Pydantic",
        "version": "2.12+",
        "purpose": "Data validation and structured outputs"
      },
      {
        "name": "PyTorch",
        "version": "2.10+",
        "purpose": "Deep learning framework"
      },
      {
        "name": "Transformers",
        "version": "5.1.0+",
        "purpose": "Hugging Face transformers library"
      },
      {
        "name": "aisuite",
        "version": "0.1.14",
        "purpose": "Unified LLM interface for multi-provider access",
        "optional": true,
        "when": "Need to switch between LLM providers or use MCP tools across providers"
      }
    ],
    "databases": [
      {
        "type": "vector",
        "name": "ChromaDB",
        "purpose": "Vector store for RAG"
      },
      {
        "type": "vector",
        "name": "FAISS",
        "purpose": "Alternative vector store"
      },
      {
        "type": "sql",
        "name": "SQLite",
        "purpose": "Prompt/state storage"
      },
      {
        "type": "sql",
        "name": "PostgreSQL",
        "purpose": "Production database"
      }
    ],
    "llmProviders": [
      {
        "name": "OpenAI",
        "models": [
          "gpt-5.2",
          "gpt-5.3-codex",
          "gpt-4o",
          "gpt-4o-mini",
          "o1",
          "o1-mini"
        ],
        "purpose": "Primary LLM"
      },
      {
        "name": "Anthropic",
        "models": [
          "claude-opus-4.6",
          "claude-3-5-sonnet-20241022",
          "claude-3-5-haiku-20241022"
        ],
        "purpose": "Alternative LLM"
      },
      {
        "name": "Google",
        "models": [
          "gemini-3-pro",
          "gemini-3-flash",
          "gemini-2.5-flash",
          "gemini-2.0-flash-exp"
        ],
        "purpose": "Long context LLM"
      },
      {
        "name": "Ollama",
        "models": [
          "llama3.3",
          "llama3.2",
          "qwen2.5",
          "mistral",
          "codellama"
        ],
        "purpose": "Local LLM"
      }
    ],
    "tools": [
      {
        "name": "pytest",
        "purpose": "Testing"
      },
      {
        "name": "ruff",
        "purpose": "Linting"
      },
      {
        "name": "mypy",
        "purpose": "Type checking"
      },
      {
        "name": "langsmith",
        "purpose": "Agent tracing and debugging"
      },
      {
        "name": "poetry",
        "purpose": "Dependency management"
      }
    ],
    "styleGuides": [
      {
        "name": "pep8",
        "description": "PEP 8 Style Guide",
        "reference": "https://pep8.org"
      },
      {
        "name": "google",
        "description": "Google Python Style",
        "reference": "https://google.github.io/styleguide/pyguide.html"
      }
    ]
  },
