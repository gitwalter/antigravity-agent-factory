    ],
    "llmConfiguration": {
      "abstractionLayer": {
        "name": "aisuite",
        "version": "0.1.14",
        "description": "Provider-agnostic LLM interface by Andrew Ng",
        "installation": "pip install 'aisuite[google,mcp]'"
      },
      "defaultProvider": "google",
      "providers": [
        {
          "name": "Google Gemini",
          "id": "google",
          "envVar": "GOOGLE_API_KEY",
          "tier": "free",
          "models": [
            {
              "id": "gemini-2.5-flash",
              "purpose": "Primary model - reasoning, analysis, complex tasks",
              "contextWindow": 1048576,
              "maxOutput": 65535
            },
            {
              "id": "gemini-2.5-flash-lite",
              "purpose": "Fast model - simple tasks, high volume",
              "contextWindow": 1048576,
              "maxOutput": 65535
            }
          ],
          "default": true
        },
        {
          "name": "Ollama",
          "id": "ollama",
          "envVar": null,
          "tier": "local",
          "models": [
            {
              "id": "llama3.2",
              "purpose": "Local development and offline"
            },
            {
              "id": "mistral",
              "purpose": "Local alternative"
            },
            {
              "id": "codellama",
              "purpose": "Code generation"
            }
          ],
          "default": false,
          "note": "Fallback for offline development"
        }
      ],
      "modelMapping": {
        "primary": "google:gemini-2.5-flash",
        "fast": "google:gemini-2.5-flash-lite",
        "local": "ollama:llama3.2",
        "code": "ollama:codellama"
      }
    },
