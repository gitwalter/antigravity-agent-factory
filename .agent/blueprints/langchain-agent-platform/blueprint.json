{
  "metadata": {
    "blueprintId": "langchain-agent-platform",
    "blueprintName": "LangChain Agent Platform Blueprint",
    "description": "Enterprise multi-domain LangChain/LangGraph agent development platform with API, trading, business, data, and RAG capabilities",
    "version": "1.0.0",
    "author": "Cursor Agent Factory",
    "tags": [
      "python",
      "ai",
      "agents",
      "langchain",
      "langgraph",
      "multi-domain",
      "enterprise",
      "aisuite",
      "gemini"
    ],
    "extends": "ai-agent-development",
    "purpose": "Enable developers to build production-grade AI agents across multiple domains with provider-agnostic LLM access"
  },
  "stack": {
    "primaryLanguage": "python",
    "pythonVersion": "^3.11",
    "frameworks": [
      {
        "name": "LangChain",
        "version": "1.2+",
        "purpose": "Core agent framework and chains"
      },
      {
        "name": "LangGraph",
        "version": "1.0+",
        "purpose": "Stateful agent orchestration and workflows"
      },
      {
        "name": "CrewAI",
        "version": "1.9+",
        "purpose": "Multi-agent crew orchestration"
      },
      {
        "name": "aisuite",
        "version": "0.1.14",
        "purpose": "Unified LLM interface - provider abstraction layer",
        "required": true,
        "note": "Andrew Ng's LLM abstraction for easy provider switching"
      },
      {
        "name": "FastAPI",
        "version": "0.128+",
        "purpose": "Production API endpoints"
      },
      {
        "name": "Streamlit",
        "version": "1.54+",
        "purpose": "Interactive dashboards and UIs"
      },
      {
        "name": "Pydantic",
        "version": "2.12+",
        "purpose": "Data validation and structured outputs"
      },
      {
        "name": "pandas",
        "version": "2.2+",
        "purpose": "Data manipulation and analysis"
      },
      {
        "name": "polars",
        "version": "1.0+",
        "purpose": "High-performance data processing"
      },
      {
        "name": "vectorbt",
        "version": "0.26+",
        "purpose": "Backtesting engine for trading"
      },
      {
        "name": "httpx",
        "version": "0.27+",
        "purpose": "Async HTTP client for API calling"
      }
    ],
    "databases": [
      {
        "type": "vector",
        "name": "ChromaDB",
        "version": "0.5+",
        "purpose": "Vector embeddings for RAG"
      },
      {
        "type": "sql",
        "name": "PostgreSQL",
        "version": "16+",
        "purpose": "Production database"
      },
